{
  "name": "ifce-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "copy-deps": "copy-newer --cwd node_modules ** ./dist/node_modules",
    "start": "node dist/server/server.js",
    "preserve": "npm run build",
    "s": "npm run serve",
    "serve.w": "concurrently \"npm run build.w\" \"nodemon --watch ./src ./dist/server ./dist/server/server.js\"",
    "serve": "npm run start",
    "lint": "tslint \"src/**/*.ts\"",
    "prebuild": "npm install",
    "b": "npm run build.w",
    "build": "npm run copy-deps && tsc",
    "build.w": "npm run copy-deps && tsc -w",
    "test-dev": "nyc --reporter=text --reporter=html mocha -r ts-node/register test/**/*.test.ts --exit",
    "test-dev-watch": "nodemon --watch . --ext ts --exec \"nyc --reporter=html --reporter=text mocha -r ts-node/register test/**/*.test.ts\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.5.32",
    "@types/multer": "^1.3.10",
    "@types/mysql": "^2.15.8",
    "@types/node": "^12.12.11",
    "@types/supertest": "^2.0.9",
    "@types/ws": "^6.0.3",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "copy-newer": "^2.1.2",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.12",
    "ws": "^7.2.0"
  }
}
